{
  // 如果您是通过uni_modules形式引入uView，可以忽略此配置
  "easycom": {
    "^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "Soumns",
        "navigationStyle": "custom", // 隐藏系统导航栏(用户自定义导航栏)
        "navigationBarTextStyle": "black" // 状态栏字体为白色，只能为 white-白色，black-黑色 二选一
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "Soumns",
        "navigationStyle": "custom", // 隐藏系统导航栏(用户自定义导航栏)
        "navigationBarTextStyle": "black" // 状态栏字体为白色，只能为 white-白色，black-黑色 二选一
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "Soumns",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  // tabBar区域
  "tabBar": {
    "color": "#C9C9C9",
    "selectedColor": "#4c7ef6",
    "backgroundColor": "#F9FAFC",
    "borderStyle": "white",
    "list": [{
        "pagePath": "pages/home/index",
        "text": "首页",
        "iconPath": "/static/uploads/tabbar-home.png",
        "selectedIconPath": "/static/uploads/tabbar-home-active.png"
      },
      {
        "pagePath": "pages/my/index",
        "text": "我的",
        "iconPath": "/static/uploads/tabbar-my.png",
        "selectedIconPath": "/static/uploads/tabbar-my-active.png"
      }
    ]
  },

  "uniIdRouter": {}
}
