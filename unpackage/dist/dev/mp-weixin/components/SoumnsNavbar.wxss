/* bg-center */
.soumns-bg.data-v-504fbaf8 {
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
}
/* flex-center  */
.soumns-flex.data-v-504fbaf8 {
  display: flex;
  align-items: center;
  justify-content: center;
}
.soumns-btn.data-v-504fbaf8 {
  width: 200px;
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 30px;
  color: #fff;
}
.container-navbar.data-v-504fbaf8 {
  width: 100%;
  background-color: #338aff;
}
.container-navbar .container-top-bg.data-v-504fbaf8 {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
}
.container-navbar .container-capsule.data-v-504fbaf8 {
  width: 100%;
  line-height: 44px;
  position: fixed;
  top: 0;
  font-size: 32rpx;
  color: #fff;
  font-weight: 500;
}
.container-navbar .container-capsule .menu_btn.data-v-504fbaf8 {
  overflow: hidden;
  display: flex;
  align-items: center;
  position: fixed;
}
.container-navbar .container-capsule .menu_btn .arrowleft.data-v-504fbaf8 {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-160%, -50%) !important;
  -webkit-transform: translate(-160%, -50%) !important;
}
.container-navbar .container-capsule .menu_btn .text_box.data-v-504fbaf8 {
  width: 1rpx;
  height: 20px;
  background-color: #ddd;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%) !important;
  -webkit-transform: translate(-50%, -50%) !important;
}
.container-navbar .container-capsule .menu_btn .home.data-v-504fbaf8 {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(60%, -50%) !important;
  -webkit-transform: translate(60%, -50%) !important;
}

